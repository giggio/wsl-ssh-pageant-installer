<service>
    <id>wsl-ssh-pageant</id>
    <name>WSL SSH Pageant</name>
    <description>A Pageant -> TCP bridge for use with WSL, allowing for Pageant to be used as an ssh-ageant within the WSL environment.</description>
    <executable>%BASE%\bin\wsl-ssh-pageant.exe</executable>
    <arguments>--winssh ssh-pageant</arguments>
    <log mode="roll-by-size">
        <sizeThreshold>8192</sizeThreshold>
        <keepFiles>4</keepFiles>
    </log>
    <logpath>%BASE%\logs\</logpath>
    <startmode>Automatic</startmode>
    <delayedAutoStart>true</delayedAutoStart>
    <interactive>true</interactive>
</service>